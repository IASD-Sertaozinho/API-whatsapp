generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    cel     String  @id @unique
    name    String
    message Message
    Admin   Admin[]
}

model Admin {
    username String @id @unique
    password String
    user     User   @relation(fields: [userCel], references: [cel])
    userCel  String
}

enum Message {
    COMUM
    MULHERES
    JOVENS
}
